{
  "headers": [
    {
      "source": "/(.*)\\.(tsx|ts|js|jsx)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!.*\\.(?:ts|tsx|js|jsx|json|png|jpg|svg|css|ico|woff2|woff|ttf)).*)",
      "destination": "/index.html"
    }
  ]
}
