{
  "headers": [
    {
      "source": "/(.*)\\.tsx",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*)\\.ts",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!.*\\.(?:ts|tsx|js|jsx|json|png|jpg|svg|css|ico|woff2|woff|ttf)).*)",
      "destination": "/index.html"
    }
  ]
}
